local library = loadstring(game.HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua", true))()
local window = library.CreateLib("Saturn 1.0 RELEASE - PRIVATE VERSION" , "DarkTheme")

-- Saturn
local Bedwars = window:NewTab("Saturn Bedwars")
local Bedwars_Section = Bedwars:NewSection("Saturn PRIVATE Bedwars")


Bedwars_Section:NewSlider("Speed", "Changes your speed", 500, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    
end)

Bedwars_Section:NewSlider("JumpPower", "Makes you jump higher", 350, 50, function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

Bedwars_Section:NewButton("Diamond Grabber", "Grabs diamonds", function()
    spawn(function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").ItemDrops.diamond.CFrame
                end)
            end
        end

Bedwars_Section:NewButton("Infinite Jump", "Jump multiple times in the air and not fall in the void", function()
   getgenv().InfiniteJump = true;
			game:GetService("UserInputService").jumpRequest:Connect(function()
				wait(0.0006)
				if getgenv().InfiniteJump == true then
					wait(0.006)
					game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass"Humanoid":ChangeState("Jumping")
					wait(0.0006)
					game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass"Humanoid":ChangeState("freefall")
				end
			end)
		else
			getgenv().InfiniteJump = false;
		end
	end

Bedwars_Section:NewButton("DragonBreathe", "Breathes Xu'rot breath at players", function()
   		while true do task.wait(0.3)
					local args = {
						[1] = {
							["player"] = game:GetService("Players").LocalPlayer
						}
					}
					game:GetService("ReplicatedStorage"):WaitForChild("rbxts_include"):WaitForChild("node_modules"):WaitForChild("@rbxts"):WaitForChild("net"):WaitForChild("out"):WaitForChild("_NetManaged"):WaitForChild("DragonBreath"):FireServer(unpack(args))
					
				end			
            end
        end

Bedwars_Section:NewButton("ConfettiPlayerAnnoyer", "Annoys other players with a annoying sound effect", function()
   			while true do task.wait(0.3)
						game:GetService("ReplicatedStorage"):WaitForChild("events-@easy-games/game-core:shared/game-core-networking@getEvents.Events"):WaitForChild("useAbility"):FireServer("PARTY_POPPER")
				end			
            end
        end



